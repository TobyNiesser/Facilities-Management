<html lang="en">
<!-- begin::Head -->
<head>
    <meta charset="utf-8" />

    <title>Estates Management Dashboard</title>
    <meta name="description" content="Latest updates and statistic charts">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <!--begin::Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700">
    <!--end::Fonts -->
    <!--begin::Page Vendors Styles(used by this page) -->
    <link href="/plugins/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Page Vendors Styles -->
    <!--begin::Global Theme Styles(used by all pages) -->
    <link href="/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="/css/style.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Global Theme Styles -->
    <!--begin::Layout Skins(used by all pages) -->
    <link href="/css/skins/header/base/light.css" rel="stylesheet" type="text/css" />
    <link href="/css/skins/header/menu/light.css" rel="stylesheet" type="text/css" />
    <link href="/css/skins/brand/light.css" rel="stylesheet" type="text/css" />
    <link href="/css/skins/aside/light.css" rel="stylesheet" type="text/css" />
    <!--end::Layout Skins -->
    <link rel="shortcut icon" href="/media/logos/favicon.ico" />
</head>
<!-- end::Head -->



<!-- begin::Body -->
<body>
    <!-- begin:: Root -->
    <!-- begin:: Subheader -->
    <div>
        <h1 class="kt-subheader__title">Facilities Management Dashboard</h1>
    </div>

    <div class="kt-subheader   kt-grid__item" id="kt_subheader">
        <div class="kt-container  kt-container--fluid ">
            <div class="kt-subheader__main">

            </div>
        </div>
    </div>
    <!-- end:: Subheader -->
    <!-- begin:: Content -->
    <div class="kt-container  kt-container--fluid  kt-grid__item kt-grid__item--fluid">
        <!--begin::Dashboard 4-->
        <div class="row">
            <div class="col">
                <div class="row">
                </div>
                <!--OFFICE PORTLET-->
                <div class="kt-portlet kt-portlet--solid-warning kt-portlet--height-fluid">
                    <div class="kt-portlet__head">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon"><i class="flaticon2-plus"></i></span>
                        </div>
                        <div class="kt-portlet__head-toolbar">
                            Configure Offices
                        </div>
                    </div>
                    <div class="kt-portlet__body">
                        <div class="kt-portlet__content">
                            <h1 class="kt-font-light">Add Office</h1>
                            <img src="~/media/logos/Office Icon.png" />
                        </div>
                    </div>
                    <div class="kt-portlet__foot kt-portlet__foot--sm kt-align-right">
                        <a href="/Offices/Index" class="btn btn-font-light btn-outline-hover-light">View</a>
                        <a href="/Offices/Create" class="btn btn-font-light btn-outline-hover-light">Add</a>
                    </div>
                </div>
                <!--end::Portlet-->
            </div>

            <div class="col">
                <!--SYSTEM PORTLET-->
                <div class="kt-portlet kt-portlet--solid-warning kt-portlet--height-fluid">
                    <div class="kt-portlet__head">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon"><i class="flaticon2-plus"></i></span>
                        </div>
                        <div class="kt-portlet__head-toolbar">
                            Configure Systems
                        </div>
                    </div>
                    <div class="kt-portlet__body">
                        <div class="kt-portlet__content">
                            <h1 class="kt-font-light">Add System</h1>
                            <img src="~/media/logos/form.png" />
                        </div>
                    </div>
                    <div class="kt-portlet__foot kt-portlet__foot--sm kt-align-right">
                        <a href="/Settings/Index" class="btn btn-font-light btn-outline-hover-light">View</a>
                        <a href="/Settings/Create" class="btn btn-font-light btn-outline-hover-light">Add</a>
                    </div>
                </div>
                <!--end::Portlet-->
            </div>


            <div class="col">
                <!--MANAGER PORTLET-->
                <div class="kt-portlet kt-portlet--solid-warning kt-portlet--height-fluid">
                    <div class="kt-portlet__head">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon"><i class="flaticon2-plus"></i></span>
                        </div>
                        <div class="kt-portlet__head-toolbar">
                            Configure Managers
                        </div>
                    </div>
                    <div class="kt-portlet__body">
                        <div class="kt-portlet__content">
                            <h1 class="kt-font-light">Add Manager</h1>
                            <img src="~/media/logos/manager icon.png" />
                        </div>
                    </div>
                    <div class="kt-portlet__foot kt-portlet__foot--sm kt-align-right">
                        <a href="/Managers/Index" class="btn btn-font-light btn-outline-hover-light">View</a>
                        <a href="/Managers/Create" class="btn btn-font-light btn-outline-hover-light">Add</a>
                    </div>
                </div>
                <!--end::Portlet-->
            </div>

            <div class="col">
                <!--SUPPLIER PORTLET-->
                <div class="kt-portlet kt-portlet--solid-warning kt-portlet--height-fluid">
                    <div class="kt-portlet__head">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon"><i class="flaticon2-plus"></i></span>
                        </div>
                        <div class="kt-portlet__head-toolbar">
                            Configure Suppliers
                        </div>
                    </div>
                    <div class="kt-portlet__body">
                        <div class="kt-portlet__content">
                            <h1 class="kt-font-light">Add Supplier</h1>
                            <img src="~/media/logos/shopping-bag_Small.png" />
                        </div>
                    </div>
                    <div class="kt-portlet__foot kt-portlet__foot--sm kt-align-right">
                        <a href="/Companies/Index" class="btn btn-font-light btn-outline-hover-light">View</a>
                        <a href="/Companies/Create" class="btn btn-font-light btn-outline-hover-light">Add</a>
                    </div>
                </div>
                <!--end::Portlet-->
            </div>

            <div class="col">
                <div class="row">
                </div>
                <!--OTHER PORTLET-->
                <div class="kt-portlet kt-portlet--solid-info kt-portlet--height-fluid">
                    <div class="kt-portlet__head">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon"><i class="flaticon2-plus"></i></span>
                        </div>
                        <div class="kt-portlet__head-toolbar">
                            Configure Others
                        </div>
                    </div>
                    <div class="kt-portlet__body">
                        <div class="kt-portlet__content">
                            <h1 class="kt-font-light">Add Other</h1>
                            <h5 class="kt-font-light">Files, Frequencies, Items, Permissionsssss, Responsibilities, Statuses.</h5>
                        </div>
                    </div>
                    <div class="kt-portlet__foot kt-portlet__foot--sm kt-align-right">
                        <div class="dropdown dropdown-inline">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="background-color: white;">
                                Add Other
                            </button>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton" style="">
                                <a class="dropdown-item" href="/Files/Index">Files</a>
                                <a class="dropdown-item" href="/Frequencies/Index">Frequencies</a>
                                <a class="dropdown-item" href="/Items/Index">Items</a>
                                <a class="dropdown-item" href="/Permissions/Index">Permissions</a>
                                <a class="dropdown-item" href="/Responsibilities/Index">Responsibilities</a>
                                <a class="dropdown-item" href="/Statuses/Index">Statuses</a>
                            </div>
                        </div>
                    </div>
                </div>
                <!--end::Portlet-->
            </div>


        </div>

        <div class="row">
            <!--CALENDAR PORTLET-->
            <div class="col-xl-12">
                <div class="kt-portlet" id="kt_portlet">
                    <div class="kt-portlet__head kt-portlet__head--lg" style="background-color: #34bfa3;">
                        <div class="kt-portlet__head-label">
                            <h1 class="kt-font-light">Event Calendar</h1>
                        </div>
                        <div class="kt-portlet__head-toolbar">
                            <div class="kt-portlet__head-group show">
                                <button type="button" class="btn btn-brand" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                    <i class="la la-plus"></i> Add Event
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="kt-portlet__body">
                        <div id="calendar"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <!-- begin:: FOOTER -->
    <!-- end:: FOOTER -->
    <!-- start::Scrolltop -->
    <div id="kt_scrolltop" class="kt-scrolltop">
        <i class="fa fa-arrow-up"></i>
    </div>
    <!-- end::Scrolltop -->
    <!-- begin::Global Config(global config for global JS sciprts) -->
    <script>
        var KTAppOptions = {
            "colors": {
                "state": {
                    "brand": "#5d78ff",
                    "metal": "#c4c5d6",
                    "light": "#ffffff",
                    "accent": "#00c5dc",
                    "primary": "#5867dd",
                    "success": "#34bfa3",
                    "info": "#36a3f7",
                    "warning": "#ffb822",
                    "danger": "#fd3995",
                    "focus": "#9816f4"
                },
                "base": {
                    "label": [
                        "#c5cbe3",
                        "#a1a8c3",
                        "#3d4465",
                        "#3e4466"
                    ],
                    "shape": [
                        "#f0f3ff",
                        "#d9dffa",
                        "#afb4d4",
                        "#646c9a"
                    ]
                }
            }
        };
    </script>
    <!-- end::Global Config -->
    <!--begin::Global Theme Bundle(used by all pages) -->
    <script src="/plugins/global/plugins.bundle.js" type="text/javascript"></script>
    <script src="/js/scripts.bundle.js" type="text/javascript"></script>
    <!--end::Global Theme Bundle -->
    <!--begin::Page Vendors(used by this page) -->
    <script src="/plugins/custom/fullcalendar/fullcalendar.bundle.js" type="text/javascript"></script>
    <!--end::Page Vendors -->
    <!--begin::Page Scripts(used by this page) -->
    <script src="/js/pages/dashboard.js" type="text/javascript"></script>

    <script>

        document.addEventListener('DOMContentLoaded', function () {
            var calendarEl = document.getElementById('calendar');

            var calendar = new FullCalendar.Calendar(calendarEl, {
                plugins: ['dayGrid', 'timeGrid', 'list'],
                events: '/Dashboard/DashboardMenu?Handler=Events'
            });

            calendar.render();
        });

    </script>

    <script>
            var calendar = new FullCalendar.Calendar(calendarEl, {
                plugins: ['bootstrap'],
                themeSystem: 'kt_calendar'
            });
    </script>

    <!--end::Page Scripts -->
</body>
<!-- end::Body -->
</html>

